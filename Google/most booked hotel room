 
// Problem Statement @: https://leetcode.com/discuss/interview-question/421787/
#include <iostream>
#include <vector>
#include <map>
using namespace std;
string mostBookedRoom(vector<string> &vec){
    map<string,int> m;
    for(string s : vec){
        if(s[0] == '+'){m[s]++;}
    }
    string temp;
    int max = -1;
    for(auto i : m){
        if(i.second > max){
            max = i.second;
            temp = i.first;
        }
    }
    return string(temp.begin() + 1, temp.end());
}
int main() {
    vector<string> vec = "+1A", "+3E", "-1A", "+4F", "+1A", "-3E"};
    cout << mostBookedRoom(vec) << endl;
    return 0;
}
